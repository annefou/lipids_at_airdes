abstract = {{\it GenX} is a versatile program using the differential evolution algorithm for fitting X-ray and neutron reflectivity data. It utilizes the Parratt recursion formula for simulating specular reflectivity. The program is easily extensible, allowing users to incorporate their own models into the program. This can be useful for fitting data from other scattering experiments, or for any other minimization problem which has a large number of input parameters and/or contains many local minima, where the differential evolution algorithm is suitable. In addition, {\it GenX} manages to fit an arbitrary number of data sets simultaneously. The program is released under the GNU General Public License.},
keywords = {X-ray reflectivity, neutron reflectivity, differential evolution algorithm, genetic algorithm},
}


@article{Gerelli2016,
author = "Gerelli, Yuri",
title = "{{\it Aurore}: new software for neutron reflectivity data analysis}",
journal = "Journal of Applied Crystallography",
year = "2016",
volume = "49",
number = "1",
pages = "330--339",
month = "Feb",
doi = {10.1107/S1600576716000108},
url = {https://doi.org/10.1107/S1600576716000108},
abstract = {{\it Aurore} is a free software application based on MATLAB scripts designed for the graphical analysis, inspection and simulation of neutron reflectivity data. Its architecture, combined with graphics and other advantages of the MATLAB environment, should allow continued development of this software and inclusion of new features and analysis methods. The development of the software was driven by the necessity for a non-commercial open-source application for the analysis of neutron reflectivity data. {\it Aurore} provides a robust and reliable method for evaluation of parameter uncertainty, a feature almost absent in similar software applications. In the present paper the main functionalities of the software are presented, together with a comprehensive description of the modeling approaches available at the moment. The code is released under a Creative Commons Attribution Non-Commercial License V2.0. The software application can be downloaded at http://aurorenr.sourceforge.net/.},
keywords = {neutron reflectivity data analysis, computer programs},
}

